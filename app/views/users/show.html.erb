<h1><%= @user.name %></h1>
<p>ノート数:<%= @user.forms.count %></p>

<% @user.forms.each do |user| %>
 <div class="container post">
      <div class="row">
          <div class="col-md-10 col-md-offset-1"><h3><%= user.title %></h3></div>
          <div class="col-md-10 col-md-offset-1"><p><%= user.created_at %></p></div>
          <div class="col-md-10 col-md-offset-1"><iframe srcdoc="<%= user.comment %>"></iframe></div>
      </div>
 </div>
<% end %>

